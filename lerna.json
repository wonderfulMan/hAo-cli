{
  "version": "0.0.68",
  "useWorkspaces": true,
  "private": true,
  "packages": [
    "packages/common/cli*",
    "packages/react/cli*",
    "packages/vue/cli*"
  ],
  "npmClient": "yarn",
  "npmClientArgs": [
    "--production"
  ],
  "command": {
    "publish": {
      "ignoreChanges": [
        "node_modules",
        "common/cli*/src",
        "vue/cli*/src",
        "react/cli*/src"
      ]
    }
  }
}
