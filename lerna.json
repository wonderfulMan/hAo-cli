{
  "version": "0.2.27",
  "useWorkspaces": true,
  "private": true,
  "packages": [
    "packages/vue/*",
    "packages/react/*",
    "packages/common/*"
  ],
  "npmClient": "yarn",
  "npmClientArgs": [
    "--production"
  ],
  "command": {
    "publish": {
      "ignoreChanges": [
        "node_modules"
      ]
    }
  }
}
